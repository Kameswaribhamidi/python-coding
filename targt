#row sum=2
n=int(input())
m=[]
x=0
for i in range(0,n):
  l=[]
  for j in range(0,n):
    x=int(input())
    l.append(x)
  m.append(l)
for i in m:
  print(i)
for i in range(0,n):
  s=0
  for j in range(0,n):
     s=s+m[i][j]
  print(s)
if s==2:
     x=x+1
print(x)        
if x==n:
  print('target matrix')
else:
  print('no')
    
